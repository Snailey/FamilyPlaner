<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_main"
    app:startDestination="@id/logInFragment">

    <activity
        android:id="@+id/mainActivity"
        android:name="no.hiof.snailey.familyplaner.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <activity
        android:id="@+id/logInActivity"
        android:name="no.hiof.snailey.familyplaner.LogInActivity"
        android:label="activity_log_in"
        tools:layout="@layout/activity_main" />
    <fragment
        android:id="@+id/shoppingListFragment"
        android:name="no.hiof.snailey.familyplaner.ShoppingListFragment"
        android:label="fragment_shopping_list"
        tools:layout="@layout/fragment_shopping_list" >
        <action
            android:id="@+id/action_shoppingListFragment_to_toDoFragment2"
            app:destination="@id/toDoFragment" />
        <action
            android:id="@+id/action_shoppingListFragment_to_calendarFragment2"
            app:destination="@id/calendarFragment" />
    </fragment>
    <fragment
        android:id="@+id/toDoFragment"
        android:name="no.hiof.snailey.familyplaner.ToDoFragment"
        android:label="fragment_to_do"
        tools:layout="@layout/fragment_to_do" >
        <action
            android:id="@+id/action_toDoFragment_to_shoppingListFragment2"
            app:destination="@id/shoppingListFragment" />
        <action
            android:id="@+id/action_toDoFragment_to_calendarFragment2"
            app:destination="@id/calendarFragment" />
    </fragment>
    <fragment
        android:id="@+id/calendarFragment"
        android:name="no.hiof.snailey.familyplaner.CalendarFragment"
        android:label="fragment_calendar"
        tools:layout="@layout/fragment_calendar" >
        <action
            android:id="@+id/action_calendarFragment_to_shoppingListFragment2"
            app:destination="@id/shoppingListFragment" />
        <action
            android:id="@+id/action_calendarFragment_to_toDoFragment2"
            app:destination="@id/toDoFragment" />
    </fragment>
    <fragment
        android:id="@+id/logInFragment"
        android:name="no.hiof.snailey.familyplaner.LogInFragment"
        android:label="fragment_log_in"
        tools:layout="@layout/fragment_log_in" >
        <action
            android:id="@+id/action_logInFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_logInFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="no.hiof.snailey.familyplaner.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" >
        <action
            android:id="@+id/action_registerFragment_to_logInFragment"
            app:destination="@id/logInFragment" />
        <action
            android:id="@+id/action_registerFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
</navigation>